<DupRoot>:
    orientation: "vertical"
    ActionBar:
        ActionView:
            ActionPrevious:
                title: ""
                with_previous: False
            ActionButton:
                text: "To Front"
                on_press: app.root.next_screen("file_screen")
            ActionButton:
                text: "Search"
                on_press: app.root.next_screen("category_screen")

    ScreenManager:
        id: dup_screen_manager
        FileScreen:
            id: file_screen
            name: "file_screen"
        CategoryScreen:
            id: category_screen
            name: "category_screen"


<CategoryScreen>:
    BoxLayout:
        orientation: "vertical"
        GridLayout:
            rows: 2
            cols: 2
            Button:
                text: "button 1"
            Button:
                text: "button 2"
            Button:
                text: "button 3"
            Button:
                text: "button 4"
        Carousel:
            id: gallery_carousel
